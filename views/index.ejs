  <!-- for dynamic data use templating -->

  <div class="container" style="margin-bottom: 10px;margin-top: 10px;">
    <%if(photolist.length){%>
    <div class="row">
      <%for(var i=0; i < photolist.length; i++){%>
      <div class="col-12 col-md-4">
        <div class="card d-flex justify-content-center"
          style="margin-bottom:10px;border-radius: 10px;background-color: white;width: auto;height: 450px;">
          <%var path=photolist[i].path%>
          <img src="<%=path%>" class="card-img-top img-fluid" alt="...">
          <div class="card-body" style="padding: 10px;">
            <h3 class="card-title"><%=photolist[i].title%></h3>
            <h5 class="card-title">Category : <span
                style="display:inline; color: blue;"><%=photolist[i].category%></span>
            </h5>
            <h5 class="card-title">
              <Caption>Caption : </Caption><span style="display:inline; color: blue;"><%=photolist[i].caption%></span>
            </h5>
          </div>
        </div>
      </div>
      <%}%>
        </div>
      <%}%>
      <div id="particles-js"></div>
    </div>

